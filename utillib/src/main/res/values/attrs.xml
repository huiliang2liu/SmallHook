<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="StripClothes">
        <attr name="imag1" format="reference" />
        <attr name="imag2" format="reference" />
    </declare-styleable>
    <declare-styleable name="FanProgress">
        <attr name="speed" format="float" />
        <attr name="period" format="float" />
        <attr name="backgroundColor" format="color" />
    </declare-styleable>
    <declare-styleable name="RecycleViewGroup">
        <attr name="inhaleDir">
            <enum name="UP" value="0" />
            <enum name="DOWN" value="1" />
            <enum name="LEFT" value="2" />
            <enum name="RIGHT" value="3" />
        </attr>
    </declare-styleable>
    <declare-styleable name="RollerView">
        <attr name="data" format="reference" />
        <attr name="select" format="integer" />
        <attr name="lineColor" format="color" />
        <attr name="textColor" format="color" />
    </declare-styleable>
    <declare-styleable name="RefreshSurfaceView">
        <attr name="refresh" format="boolean" />
    </declare-styleable>
    <declare-styleable name="WaitView">
        <attr name="frequency" format="integer" />
        <attr name="displacement" format="integer" />
        <attr name="color" format="color" />
        <attr name="radiusDifference" format="integer" />
        <attr name="waitViewRefresh" format="boolean" />
    </declare-styleable>
</resources>
